{
  "name": "stellar-ui",
  "private": true,
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "generate": "yarn run generate:gql && yarn run generate:grpc",
    "generate:gql": "graphql-codegen --config graphqlcodegen.yml",
    "generate:grpc": "mkdir -p src/grpc/jwtks && protoc -I ../../api/grpc --ts_out=src/grpc/jwtks ../../api/grpc/jwtks.proto",
    "dev": "next dev",
    "dev:watch": "concurrently \"yarn run dev\" \"yarn run generate --watch\"",
    "build": "next build",
    "start": "next start",
    "style:all": "yarn run style:lint && yarn run style:prettier&& yarn run style:ts",
    "style:lint": "next lint",
    "style:ts": "yarn run tsc -t es6",
    "style:prettier": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "make-pretty": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "test": "jest --watch",
    "test:ci": "jest --passWithNoTests --coverage",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless"
  },
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "@apollo/react-hooks": "^4.0.0",
    "@grpc/grpc-js": "^1.6.8",
    "@headlessui/react": "^1.6.4",
    "@sentry/nextjs": "^7.9.0",
    "@sentry/tracing": "^7.9.0",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "google-protobuf": "^3.20.1-rc.1",
    "graphql": "^16.3.0",
    "graphql-request": "^4.3.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.7.0",
    "grpc-tools": "^1.11.2",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "isomorphic-unfetch": "^3.1.0",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.4",
    "next": "^12.2.1",
    "next-auth": "^4.10.3",
    "protoc-gen-ts": "^0.8.2",
    "react": "17",
    "react-burger-menu": "^3.0.6",
    "react-dom": "17",
    "sass": "^1.45.0",
    "twemoji": "^14.0.2",
    "yaml": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@graphql-codegen/cli": "^2.9.1",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-operations": "2.3.7",
    "@graphql-codegen/typescript-react-apollo": "3.2.11",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.0.4",
    "@testing-library/user-event": "^14.3.0",
    "@types/google-protobuf": "^3.15.6",
    "@types/node": "18.6.3",
    "@types/react": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "concurrently": "^7.2.1",
    "cypress": "^10.1.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "12.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.1.0",
    "postcss": "^8.4.13",
    "prettier": "2.6.2",
    "pretty-quick": "^3.1.3",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.22",
    "ts-node": "^10.8.1",
    "typescript": "4.7.4",
    "yaml-loader": "^0.7.0"
  },
  "resolutions": {
    "@apollo/client": "^3.5.10",
    "webpack": "^5"
  }
}
