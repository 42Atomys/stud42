{
  "name": "stellar-ui",
  "private": true,
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "generate": "graphql-codegen --config graphqlcodegen.yml",
    "dev": "next dev",
    "dev:watch": "concurrently \"yarn run dev\" \"yarn run generate --watch\"",
    "build": "next build",
    "start": "next start",
    "style:all": "yarn run style:lint && yarn run style:prettier",
    "style:lint": "next lint",
    "style:prettier": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "make-pretty": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "test": "jest --watch",
    "test:ci": "jest --passWithNoTests --coverage",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@apollo/react-hooks": "^4.0.0",
    "@panva/hkdf": "^1.0.1",
    "@sentry/nextjs": "^6.19.6",
    "@sentry/tracing": "^6.19.6",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "graphql": "^16.3.0",
    "graphql-request": "^4.2.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.6.4",
    "grpc": "^1.24.11",
    "grpc-tools": "^1.11.2",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "isomorphic-unfetch": "^3.1.0",
    "jose": "^4.6.1",
    "js-cookie": "^3.0.1",
    "next": "12.1.4",
    "next-auth": "^4.0.5",
    "react": "17.0.2",
    "react-burger-menu": "^3.0.6",
    "react-dom": "^17.0.2",
    "sass": "^1.45.0",
    "twemoji": "^14.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-react-apollo": "3.2.11",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/dom": "^8.12.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.0.4",
    "@types/node": "17.0.23",
    "@types/react": "^17.0.43",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "concurrently": "^7.1.0",
    "cypress": "^9.5.3",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "postcss": "^8.4.6",
    "prettier": "2.6.1",
    "pretty-quick": "^3.1.3",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.22",
    "ts-node": "^10.7.0",
    "typescript": "4.6.3"
  },
  "resolutions": {
    "@apollo/client": "^3.5.10",
    "webpack": "^5"
  }
}
